@ECHO OFF
SETLOCAL

REM Obtener el directorio actual
SET "dp0=%~dp0"

REM Verificar si node.exe existe en el directorio actual
IF EXIST "%dp0%\node.exe" (
    SET "_prog=%dp0%\node.exe"
) ELSE (
    SET "_prog=node"
    SET PATHEXT=%PATHEXT:;.JS;=;%"
)

REM Ejecutar el script mime/cli.js con node
"%_prog%" "%dp0%\..\mime\cli.js" %*

ENDLOCAL
EXIT /b
